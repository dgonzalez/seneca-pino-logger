  "dependencies": {
    "pino": "2.8.3",
    "seneca-log-filter": "^0.1.0"
  },
  "devDependencies": {
    "code": "3.0.x",
    "eslint-config-seneca": "3.x.x",
    "eslint-plugin-hapi": "4.x.x",
    "eslint-plugin-standard": "2.x.x",
    "lab": "11.0.x",
    "pre-commit": "1.1.x",
    "docco": "0.7.x",
    "seneca": "plugin"
  },
  "files": [
    "pino.js",
    "README.md",
    "LICENSE"
  ],
  "pre-commit": [
    "test"
